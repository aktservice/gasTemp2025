{
  "name": "gastemp2025",
  "version": "0.0.1",
  "description": "vanilla template",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview app/front",
    "cpfappsscript": "node -e \"require('fs').cpSync('./app/frontend/src/appsscript.json', './dist/appsscript.json',{recursive:true})\"",
    "cpbappsscript": "node -e \"require('fs').cpSync('./app/backend/src/appsscript.json', './dist/appsscript.json',{recursive:true})\"",
    "cpfstatic": "node -e \"require('fs').cpSync('./app/frontend/src/static', './dist',{recursive:true})\"",
    "cpbstatic": "node -e \"require('fs').cpSync('./app/backend/src/static', './dist',{recursive:true})\"",
    "buildfrontend": "vite build && npm run cpfstatic && cpfappsscript",
    "buildbackend": "node esbuild.js && npm run cpbappsscript && npm run cpbstatic",
    "build": "npm run buildfrontend && npm run buildbackend",
    "push": "clasp push",
    "open": "clasp open-script",
    "deploy": "npm run build && npm run push",
    "deployfront": "npm run run buildfrontend && npm run push",
    "deploybackend": "npm run run buildbackend && npm run push"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "@types/google-apps-script": "^1.0.97",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.0",
    "esbuild": "^0.25.5",
    "esbuild-gas-plugin": "^0.9.0",
    "jest": "^29.0.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "tailwindcss": "^4.1.8",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-plugin-singlefile": "^2.2.0"
  }
}
